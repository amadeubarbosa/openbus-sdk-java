descriptors = {
  { name = "jacorb-2.3.0",
    url = "http://www.tecgraf.puc-rio.br/~openbus/repository/jacorb-2.3.0.tar.gz",
    build = {
      type = "mavenimport",
      src = PRODAPP.."/jacorb-2.3.0"
    },
    parameters = { 
      ["lib/jacorb.jar"] = { groupId="org.jacorb", artifactId="jacorb",
          version="2.3.0", pomFile="jacorb-pom.xml" },
      ["lib/idl.jar"] = { groupId="org.jacorb",
          artifactId="jacorb-idl-compiler", version="2.3.0",
          pomFile="jacorb-idl-compiler-pom.xml" },
    },
  },
  { name = "openbus-api",
    build = {
      type = "maven",
      src = SVNDIR,      
    },
    install_files = {
      ["target/openbus-api-*.jar"] = "lib",
      ["target/dependency/*.jar"] = "lib",
    },
  },
  { name = "openbus-api-javadoc",
    build = {
      type = "command",
      src = SVNDIR,
      cmd = "mvn",
      rebuild = "clean",
      arguments = "javadoc:javadoc -Dquiet",
    },
    install_files = {
      ["target/site"] = "doc",
    },
  },
  { name = "openbus-demo-hello",
    build = {
      type = "maven",
      src = SVNDIR.."/demo/hello",
    },
    install_files = {
      ["target/openbus-demo-hello-*.jar"] = "demo/java/hello",
      ["target/dependency/*.jar"] = "lib",
    },
    dev_files = {
      ["resources"] = "demo/java/hello",
      ["src"] = "demo/java/hello",
      ["idl"] = "demo/java/hello",
      ["management"] = "demo/java/hello",
    },
  },
  { name = "openbus-demo-eventsink",
    build = {
      type = "maven",
      src = SVNDIR.."/demo/eventSink",
    },
    install_files = {
      ["target/openbus-demo-eventsink-*.jar"] = "demo/java/eventSink",
      ["target/dependency/*.jar"] = "lib",
    },
    dev_files = {
      ["resources"] = "demo/java/eventSink",
      ["src"] = "demo/java/eventSink",
      ["idl"] = "demo/java/eventSink",
      ["management"] = "demo/java/eventSink",
    },
  },
  { name = "openbus-demo-delegate",
    build = {
      type = "maven",
      src = SVNDIR.."/demo/delegate",
    },
    install_files = {
      ["target/openbus-demo-delegate-*.jar"] = "demo/java/delegate",
      ["target/dependency/*.jar"] = "lib",
    },
    dev_files = {
      ["resources"] = "demo/java/delegate",
      ["src"] = "demo/java/delegate",
      ["idl"] = "demo/java/delegate",
      ["management"] = "demo/java/delegate",
    },
  },
}
